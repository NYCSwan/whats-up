{
  "name": "whats-up",
  "version": "1.0.0",
  "description": "a whats app clone in react",
  "main": "index.js",
  "engines": {
    "node": "6.10.0",
    "npm": "3.10.10"
  },
  "scripts": {
    "test": "mocha ./test-root.js ./src/**/_tests_/*.tests.js --compilers js:babel-core/register",
    "build-api": "rm -Rf dist/api && rm -Rf dist/core && babel src/api -d dist/api && babel src/core -d dist/core",
    "babel-node": "babel-node",
    "nodemon": "nodemon --exec npm run babel-node",
    "postinstall": "npm run build-api",
    "build-spa": "rm -R dist/spa && gulp build-spa",
    "spa-server": "gulp spa-server",
    "eslint": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NYCSwan/whats-up.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NYCSwan/whats-up/issues"
  },
  "homepage": "https://github.com/NYCSwan/whats-up#readme",
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "body-parser": "^1.17.1",
    "cors": "^2.8.1",
    "css-loader": "^0.27.3",
    "express": "^4.15.0",
    "flux": "^3.1.2",
    "jsonwebtoken": "^7.1.9",
    "key-mirror": "^1.0.1",
    "node-sass": "^3.13.1",
    "redis": "^2.6.5",
    "sass-loader": "^4.1.1",
    "socket.io": "^1.7.3",
    "style-loader": "^0.13.2",
    "url-join": "^1.1.0",
    "validator": "^6.1.0"
  },
  "devDependencies": {
    "babel-loader": "^6.4.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "eslint": "^3.16.1",
    "eslint-plugin-react": "^6.10.0",
    "gulp": "^3.9.1",
    "gulp-mocha": "^4.1.0",
    "jquery": "^3.1.1",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
